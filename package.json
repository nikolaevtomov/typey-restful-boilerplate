{
  "name": "typey-restful-boilerplate",
  "version": "0.0.1",
  "description": "a better typescript restful api project",
  "main": "src/index.ts",
  "scripts": {
    "start": "node bundle.js",
    "start:dev": "cross-env NODE_ENV=development DEBUG=true webpack --watch --mode development",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:js": "cross-env NODE_ENV=production DEBUG=false webpack --progress --colors",
    "build:all": "npm run build:clean && npm run build:js",
    "build": "npm run build:all",
    "prettier": "prettier --write '*/*.{ts,tsx,js,jsx,css,less,json}'",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "tsc --noEmit && eslint 'src/*.{ts,tsx,js,jsx,css,less,json}' --quiet --fix",
    "test": "mocha -require ts-node/register 'src/**/*.spec.{ts,tsx,js,jsx}'"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/express": "^4.17.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-const-enum": "0.0.5",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.2.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:nikolaevtomov/typey-restful-boilerplate.git"
  }
}
